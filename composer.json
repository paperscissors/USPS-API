{
    "name": "paperscissorsandglue/usps-api",
    "description": "PHP USPS API integration on PHP based on original library by Vincent Gabriel.",
    "keywords": [
        "paperscissorsandglue",
        "USPS-API"
    ],
    "homepage": "https://github.com/paperscissorsandglue/USPS-API",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Filepp",
            "email": "paperscissorsandglue@pm.me",
            "homepage": "https://paperscissorsandglue.coms",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "mmucklo/krumo": "^0.6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Paperscissorsandglue\\USPS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paperscissorsandglue\\USPS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
